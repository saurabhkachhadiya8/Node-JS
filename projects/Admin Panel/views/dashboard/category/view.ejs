                                                                            class="popper-box rounded-md border border-slate-150 bg-white py-1.5 font-inter dark:border-navy-500 dark:bg-navy-700">
                                                                            <ul>
                                                                                <li>
                                                                                    <input
                                                                                        class="flex items-center px-3 h-8 pr-12 font-medium tracking-wide outline-none transition-all hover:bg-slate-100 hover:text-error focus:bg-slate-100 focus:text-error dark:hover:bg-navy-600 dark:hover:text-error dark:focus:bg-navy-600 dark:focus:text-error w-full text-left"
                                                                                        style="cursor: pointer;"
                                                                                        type="submit" name="deactivate"
                                                                                        value="Deactivate">
                                                                                </li>
                                                                                <li>
                                                                                    <input
                                                                                        class="flex items-center px-3 h-8 pr-12 font-medium tracking-wide outline-none transition-all hover:bg-slate-100 hover:text-success focus:bg-slate-100 focus:text-success dark:hover:bg-navy-600 dark:hover:text-success dark:focus:bg-navy-600 dark:focus:text-success w-full text-left"
                                                                                        style="cursor: pointer;"
                                                                                        type="submit" name="activate"
                                                                                        value="Activate">
                                                                                </li>
                                                                            </ul>
                                                                            <div
                                                                                class="my-1 h-px bg-slate-150 dark:bg-navy-500">
                                                                            </div>
                                                                            <input
                                                                                class="flex items-center px-3 h-8 pr-12 font-medium text-error tracking-wide outline-none transition-all hover:bg-slate-100 focus:bg-slate-100 dark:hover:bg-navy-600 dark:focus:bg-navy-600 w-full"
                                                                                style="cursor: pointer;" type="submit"
                                                                                name="deleteCat"
                                                                                value="Delete Category">
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="tbodySubcategory">
                                                        <% subcategories.map((subcat, index)=> { %>
                                                            <li x-data="{expanded:false}">
                                                                <div tabindex="0"
                                                                    :class="expanded && 'text-slate-800 dark:text-navy-100'"
                                                                    class="flex items-center space-x-1.5 rounded tracking-wide outline-none transition-all hover:bg-slate-100 hover:text-slate-800 focus:bg-slate-100 focus:text-slate-800 dark:hover:bg-navy-600 dark:hover:text-navy-100 dark:focus:bg-navy-600 dark:focus:text-navy-100">
                                                                    <button type="button" @click="expanded = !expanded"
                                                                        class="btn size-5 rounded-lg p-0 hover:bg-slate-300/20 focus:bg-slate-300/20 active:bg-slate-300/25 dark:hover:bg-navy-300/20 dark:focus:bg-navy-300/20 dark:active:bg-navy-300/25 h-12">
                                                                        <svg xmlns="http://www.w3.org/2000/svg"
                                                                            class="size-4.5 transition-transform"
                                                                            :class="expanded && 'rotate-90'"
                                                                            viewBox="0 0 20 20" fill="currentColor">
                                                                            <path fill-rule="evenodd"
                                                                                d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z"
                                                                                clip-rule="evenodd"></path>
                                                                        </svg>
                                                                    </button>
                                                                    <div class="tr flex items-center border border-transparent border-b-slate-200 dark:border-b-navy-500 relative w-full"
                                                                        key="<%= subcat?._id %>" style="margin: 0;">
                                                                        <a href="/dashboard/create_subcategory?editid=<%= subcat?._id %>"
                                                                            class="z-10 absolute w-full h-full"></a>
                                                                        <div class="td whitespace-nowrap rounded-l-lg px-4 py-3 sm:px-5 text-center"
                                                                            style="width: 4%;">
                                                                            <label
                                                                                class="inline-flex items-center space-x-2 z-10 relative">
                                                                                <input
                                                                                    class="subcategoryCheckbox form-checkbox is-basic size-5 rounded border-slate-400/70 checked:bg-primary checked:border-primary hover:border-primary focus:border-primary dark:border-navy-400 dark:checked:bg-accent dark:checked:border-accent dark:hover:border-accent dark:focus:border-accent"
                                                                                    type="checkbox"
                                                                                    name="category_checkbox"
                                                                                    value="<%= subcat?._id %>" />
                                                                            </label>
                                                                        </div>
                                                                        <div class="td whitespace-nowrap px-4 py-3 sm:px-5 text-center"
                                                                            style="width: 6%;">
                                                                            <span class="z-10 relative">
                                                                                <%= ++index %>
                                                                            </span>
                                                                        </div>
                                                                        <div class="td whitespace-nowrap px-4 py-3 sm:px-5"
                                                                            style="width: 10%;"></div>
                                                                        <div class="td whitespace-nowrap px-4 py-3 sm:px-5"
                                                                            style="width: 45%;">
                                                                            <a href="/dashboard/create_subcategory?editid=<%= subcat?._id %>"
                                                                                class="z-10 relative hover:underline">
                                                                                <%= subcat?.title %>
                                                                            </a>
                                                                        </div>
                                                                        <div class="td whitespace-nowrap px-4 py-3 sm:px-5"
                                                                            style="width: 15%;">
                                                                            <span class="z-10 relative">
                                                                                <%= subcat?.description.slice(0, 40) %>
                                                                                    ...
                                                                            </span>
                                                                        </div>
                                                                        <div class="td whitespace-nowrap rounded-r-lg px-4 py-3 sm:px-5 text-center"
                                                                            style="width: 20%;">
                                                                            <% if (subcat?.status==="deactive" ) { %>
                                                                                <a href="/dashboard/subcategory_status?statusid=<%= subcat?._id %>&status=deactive"
                                                                                    class="btn min-w-[7rem] bg-error font-medium text-white hover:bg-error-focus focus:bg-error-focus active:bg-error-focus/90 dark:bg-accent dark:hover:bg-accent-focus dark:focus:bg-accent-focus dark:active:bg-accent/90 z-10 relative">Deactive</a>
                                                                                <% } else { %>
                                                                                    <a href="/dashboard/subcategory_status?statusid=<%= subcat?.id %>&status=active"
                                                                                        class="btn min-w-[7rem] bg-success font-medium text-white hover:bg-success-focus focus:bg-success-focus active:bg-success-focus/90 dark:bg-accent dark:hover:bg-accent-focus dark:focus:bg-accent-focus dark:active:bg-accent/90 z-10 relative">Active</a>
                                                                                    <% } %>
                                                                        </div>
